[[workflows.workflow]]
name = "Development"
author = 49972067
mode = "parallel"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd backend && lsof -ti:3000 | xargs kill -9 2>/dev/null; npm run dev"

[[workflows.workflow.tasks]]
